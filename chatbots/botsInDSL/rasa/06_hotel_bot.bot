	}
	text response utter_juniorsuite_details_text :
		inputs in en {
			"An exclusive 400 sqft room that redefines luxury with two bedrooms, walk-in wardrobes, entertainment area, a kitchenette, private balcony and unparalleled views of the City. AMENITIES55” Smart IPTVBathrooms with walk-in shower and bathtubHairdryerCoffee MachineRefrigerator,Microwave Oven,Toaster Iron/ironing boardIn-room laptop-sized electronic safeBuilt-In Induction Hob with a Built-In HoodComplimentary Wi-Fi & Two Way Airport TransfersAccess to boardroom, and Grand Club serving breakfast and evening refreshmentsComplimentary bottled mineral water."
	}

flows :
	- user greet => chatbot utter_greet_text {
		=> user nearest_attraction => chatbot action_nearest_attractions_empty {
			=> user marine_drive => chatbot utter_marine_text ;
			=> user mattancherry => chatbot utter_mattancherry_text ;
			=> user fort_kochi => chatbot utter_fortkochi_text ;
		} ;
		=> user book_rooms => chatbot action_book_rooms_details_empty , action_book_rooms_empty {
			=> user dulex_room_details => chatbot utter_dulexroom_details_text ;
			=> user junior_suite_details => chatbot utter_juniorsuite_details_text ;
			=> user club_suite_details => chatbot utter_clubsuite_details_text ;
			=> user book_room_now => chatbot action_book_rooms_details_empty ;
		} ;
		=> user faq => chatbot utter_faq_prompt_text {
			=> user faqbreakfast => chatbot utter_faqbreakfast_text ;
			=> user faqcancellationpolicy => chatbot utter_cancellationpolicy_text ;
			=> user faqcheckin => chatbot utter_checkintime_text ;
			=> user faqcheckinage => chatbot utter_miminumage_text ;
			=> user faqcheckout => chatbot utter_checkout_text ;
			=> user faqchildrenage => chatbot utter_childrenage_text ;
			=> user faqearlycheckin => chatbot utter_earlycheckin_text ;
			=> user faqlatecheckin => chatbot utter_latecheckin_text ;
			=> user faqroomcount => chatbot utter_roomcount_text ;
			=> user faqbabycot => chatbot utter_babycot_text ;
			=> user faqbreakfasttime => chatbot utter_breakfasttime_text ;
			=> user faqrefundable => chatbot utter_refundable_text ;
			=> user faqreceptiontime => chatbot utter_receptiontime_text ;
			=> user faqparking => chatbot utter_carparking_text ;
			=> user faqparkingcost => chatbot utter_parkingcost_text ;
			=> user faqpartialcancel => chatbot utter_partialcancel_text ;
			=> user faqpetpolicy => chatbot utter_petpolicy_text ;
			=> user faqreservaconfirm => chatbot utter_reservationconfirm_text ;
			=> user faqnearestairport => chatbot utter_nearestairport_text ;
			=> user faqmodifyreservation => chatbot utter_modifyreservation_text ;
			=> user faqmeetingroom => chatbot utter_meetingroom_text ;
			=> user faqsecurity => chatbot utter_security_text ;
			=> user faqluggage => chatbot utter_luggage_text ;
			=> user faqcontactnumber => chatbot utter_contactnumber_text ;
		} ;
	} ;
	- user faqlatecheckout => chatbot utter_latecheckout_text ;

